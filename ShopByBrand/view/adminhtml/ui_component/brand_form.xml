<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">moonstarcz_course_moonstarcz_class_form.moonstarcz_class_form_data_source</item>
		</item>
		<item name="label" xsi:type="string" translate="true">General Information</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
	</argument>
	<settings>
		<buttons>
			<button name="back" class="Moonstarcz\Course\Block\Adminhtml\Moonstarcz\Class\Edit\BackButton"/>
			<button name="delete" class="Moonstarcz\Course\Block\Adminhtml\Moonstarcz\Class\Edit\DeleteButton"/>
			<button name="save" class="Moonstarcz\Course\Block\Adminhtml\Moonstarcz\Class\Edit\SaveButton"/>
			<button name="save_and_continue" class="Moonstarcz\Course\Block\Adminhtml\Moonstarcz\Class\Edit\SaveAndContinueButton"/>
		</buttons>
		<namespace>moonstarcz_course_moonstarcz_class_form</namespace>
		<dataScope>data</dataScope>
		<deps>
			<dep>moonstarcz_course_moonstarcz_class_form.moonstarcz_class_form_data_source</dep>
		</deps>
	</settings>
	<dataSource name="moonstarcz_class_form_data_source">
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
		<settings>
			<submitUrl path="*/*/save"/>
		</settings>
		<dataProvider name="moonstarcz_class_form_data_source" class="Moonstarcz\Course\Model\Moonstarczclass\DataProvider">
			<settings>
				<requestFieldName>moonstarcz_class_id</requestFieldName>
				<primaryFieldName>moonstarcz_class_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<fieldset name="general">
		<settings>
			<label>General</label>
		</settings>
		<field name="status" formElement="checkbox" sortOrder="10">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">moonstarcz_class</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">status</label>
				<dataScope>status</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="name" formElement="textarea" sortOrder="20">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">moonstarcz_class</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">name</label>
				<dataScope>name</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="image" formElement="textarea" sortOrder="30">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">moonstarcz_class</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">image</label>
				<dataScope>image</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="created_at" formElement="date" sortOrder="40">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">moonstarcz_class</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">created_at</label>
				<dataScope>created_at</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="updated_at" formElement="date" sortOrder="50">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">moonstarcz_class</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">updated_at</label>
				<dataScope>updated_at</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
	</fieldset>
</form>